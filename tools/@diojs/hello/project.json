{
  "name": "@diojs/hello",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "tools/@diojs/hello/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/tools/@diojs/hello",
        "main": "tools/@diojs/hello/src/index.ts",
        "tsConfig": "tools/@diojs/hello/tsconfig.lib.json",
        "assets": [
          "tools/@diojs/hello/*.md",
          {
            "input": "./tools/@diojs/hello/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./tools/@diojs/hello/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./tools/@diojs/hello",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./tools/@diojs/hello",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "tools/@diojs/hello/jest.config.ts"
      }
    }
  }
}
